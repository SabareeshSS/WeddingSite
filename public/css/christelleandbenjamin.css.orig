}
.lieux .bg-trait2 {
  width: 260px;
  margin: auto;
  background: url(../img/perso_trait_2.png) no-repeat 0px 50%;
}
#musique .feedback,
#rsvp .feedback {
  display: inline-table;
  margin-bottom: 0;
  padding-bottom: 0;
}
