@import url(http://fonts.googleapis.com/css?family=Anaheim);
.right {
  text-align: right!important;
}
body {
  font-family: 'Anaheim';
  line-height: 12px;
  padding: 0;
  margin: auto;
  line-height: 20px;
  color: #797979;
  font-size: 13px;
}
.navbar {
  background: url('../img/bck_header.png') fixed repeat-x;
  z-index: 1000;
}
.navbar .btn-navbar {
  margin-right: 30px;
}
.navbar .navbar-inner {
  background: none;
  -webkit-box-shadow: none;
  box-shadow: none;
  border: none;
}
.navbar .navbar-inner ul.nav {
  margin: 0 10px 0 100px;
}
.navbar .navbar-inner ul.nav li > a {
  line-height: 64px;
  margin: 0;
  color: #F9847A;
}
.navbar .navbar-inner ul.nav li.active a,
.navbar .navbar-inner ul.nav li:hover a {
  background: #f9c7c2;
  color: #000;
}
@media (max-width: 979px) {
  .navbar-fixed-top {
    margin-bottom: 0;
    background: url('../img/bck-general.jpg') repeat;
  }
  .navbar-fixed-top ul {
    margin: 0 10px 0 10px!important;
  }
  .navbar-fixed-top li {
    width: 99%;
  }
  .palette {
    text-align: center!important;
  }
}
.container-fluid {
  background: url('../img/bck-general.jpg') repeat;
  padding: 20px;
}
.container-fluid > * {
  max-width: 900px;
  margin: auto;
}
.container-fluid.white {
  background: url('../img/bck-zone-blanche.jpg') repeat;
}
p {
  text-align: justify;
}
input {
  margin: 0;
}
.success {
  color: green;
}
.error {
  color: red;
}
.btnSubmit {
  border: none;
  padding: 0;
  margin: 0;
  height: 31px;
  width: 32px;
  cursor: pointer;
  background: url(../img/form_btn.png) no-repeat 0 0;
  display: inline-block;
  text-decoration: none;
  vertical-align: middle;
  margin-top: -3px;
}
.btnSubmit:hover {
  background-position: -33px 0;
}
.left {
  text-align: left!important;
}
.guillemet {
  float: left;
  margin-top: -14px;
}
.photo,
.feedback {
  margin: auto!important;
